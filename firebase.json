{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5010
    },
    "ui": {
      "port": 4010
    },
    "hub": {
      "port": 4410
    },
    "logging": {
      "port": 4510
    },
    "eventarc": {
      "port": 9310
    },
    "tasks": {
      "port": 9510
    }
  }
}
